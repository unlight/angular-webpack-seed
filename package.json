{
  "name": "angular-webpack-seed",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "XNet",
  "scripts": {
    "gulp": "ts-node -F node_modules/gulp/bin/gulp",
    "prestart": "npm run gulp -- server:prestart",
    "start": "npm run server",
    "eslint": "eslint src/**/*.ts",
    "clean": "rm -rf build",
    "compile": "tsc",
    "compile:w": "tsc --watch",
    "tschk": "echo tsc --noEmit ... && tsc --noEmit --noUnusedLocals",
    "tschk:w": "npm run tschk -- --watch",
    "test": "npm run build:all && npm run eslint && npm run tschk && npm run coverage",
    "t": "npm run karma -- --single-run",
    "tests": "npm run t",
    "tests:w": "npm run karma",
    "server": "webpack-dev-server --hot --progress --colors",
    "server:dashboard": "npm run server -- --env.dashboard",
    "build": "webpack --progress --colors",
    "build:prod": "npm run build -- --env.prod --display-modules",
    "dashboard": "webpack-dashboard -- npm run server:dashboard",
    "up": "node node_modules/updtr/bin/updtr --test-stdout --save-exact -R simple --test \"npm run test\"",
    "build:vendor-libs": "npm run build -- --env.vendorLibs --display-modules",
    "build:vendor-style": "npm run build -- --env.vendorStyle --display-modules",
    "build:vendors": "npm run build:vendor-libs && npm run build:vendor-style",
    "build:all": "npm run build:vendors && npm run build",
    "postinstall": "npm run build:vendors",
    "yi": "yarn install --no-lockfile",
    "karma": "karma start",
    "coverage": "npm run karma -- --single-run --coverage",
    "coverage:debug": "npm run karma coverage -- --log-level debug",
    "p": "concurrently -r",
    "dev2a": "npm run p -q -- \"npm run tests:w\" \"npm run gulp eslint:watch\"",
    "dev3a": "npm run p -q -- \"npm run tests:w\" \"npm run gulp eslint:watch\" \"npm run tschk:w\"",
    "dev4a": "npm run p -q -- \"npm run tests:w\" \"npm run gulp eslint:watch\" \"npm run tschk:w\" \"npm run server\"",
    "dev2b": "npm run p -q -- \"npm run tests:w\" \"npm run server\""
  },
