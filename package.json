{
  "name": "angular-webpack-seed",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "gulp": "ts-node -F node_modules/gulp/bin/gulp",
    "start": "npm run server",
    "eslint": "eslint src/**/*.ts",
    "clean": "rm -rf build",
    "compile": "tsc",
    "compile:w": "tsc --watch",
    "tschk": "tsc --noEmit --noUnusedLocals",
    "test": "npm run build && npm run eslint && npm run tschk -q && npm run coverage",
    "t": "npm run karma -- --single-run",
    "tests:w": "npm run karma",
    "server": "webpack-dev-server --hot --inline --progress --colors",
    "server:dashboard": "npm run server -- --env.dashboard",
    "build": "webpack --progress --colors",
    "build:prod": "npm run build -- -p",
    "dashboard": "webpack-dashboard -- npm run server:dashboard",
    "up": "updtr --test-stdout --save-exact -R simple --test \"npm run test\"",
    "build:vendor-libs": "npm run build -- --env.vendorLibs",
    "build:vendor-style": "npm run build -- --env.vendorStyle",
    "postinstall": "npm run build:dll",
    "yi": "yarn install --no-lockfile",
    "karma": "karma start",
    "coverage": "npm run karma -- --single-run --coverage",
    "coverage:debug": "npm run karma coverage -- --log-level debug"
  },
  "license": "MIT",
  "devDependencies": {
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@blueprintjs/core": "1.13.0",
    "@types/jasmine": "2.5.47",
    "@types/karma": "0.13.34",
    "@types/lodash": "4.14.61",
    "@types/node": "7.0.12",
    "@types/webpack": "2.2.14",
    "@types/webpack-env": "1.13.0",
    "autoprefixer": "6.7.7",
    "awesome-typescript-loader": "3.1.2",
    "css-entry-webpack-plugin": "1.0.0-beta.4",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-plugin-typescript": "0.1.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fmt-obj": "1.3.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-nightmare": "0.4.5",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "lodash": "4.17.4",
    "node-sass": "4.5.2",
    "postcss": "5.2.16",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "postcss-url": "5.1.2",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "ts-node": "3.0.2",
    "tslib": "1.6.0",
    "typescript": "2.2.2",
    "typescript-eslint-parser": "2.0.0",
    "url-loader": "0.5.8",
    "webpack": "2.3.2",
